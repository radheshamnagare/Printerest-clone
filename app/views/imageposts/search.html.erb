<h4>Search Result</h4>
<br/><br/>
<div class="card">
 <div class="card-body">
 <div id="imageposts">
  <%if @imageposts.count == 0%> 
    <h2>Not Available</h2>
  <%else%>
  <% @imageposts.each do |imagepost| %>
   <div class="clm">
    <%= render imagepost %>
    <p>
      <%= link_to "Show this imagepost", imagepost ,class:"btn btn-dark btn-sm"%>
    </p>
    </div>
  <% end %>
  <%end%>
 </div>
</div>
</div>